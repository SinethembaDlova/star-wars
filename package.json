{
  "name": "star-wars",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.26.9",
    "@babel/eslint-parser": "^7.26.8",
    "@eslint/eslintrc": "^3.3.0",
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "react-scripts": "^5.0.1",
