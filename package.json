      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-react": "^7.26.3",
    "@eslint/compat": "^1.2.7",
    "@testing-library/dom": "^10.4.0",
